{
  "name": "sbstr8-cli",
  "version": "0.0.2",
  "description": "CLI for substrate blog basis",
  "main": "dist/cli.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc; chmod +x ./dist/cli.js",
    "dev": "npx tsc --watch",
    "type-check": "npx tsc --noEmit",
    "lint": "npx eslint --ignore-path .gitignore \"src/**/*.+(ts|js)\"",
    "format": "npx prettier --ignore-path .gitignore \"src/**/*.+(ts|js)\" --write",
    "postinstall": "npx husky install",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmarsto/sbstr8-cli.git"
  },
  "keywords": [
    "substrate",
    "sbstr8",
    "cli"
  ],
  "author": "@elmarsto",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/elmarsto/sbstr8-cli/issues"
  },
  "homepage": "https://github.com/elmarsto/sbstr8-cli#readme",
  "lint-staged": {
    "./src/**/*.{ts,js}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@types/figlet": "1.5.5",
    "commander": "10.0.1",
    "date-fns": "2.29.3",
    "extract-mdx-metadata": "2.0.0",
    "figlet": "1.6.0",
    "gray-matter": "4.0.3",
    "immer": "^10.0.1",
    "inquirer": "9.2.0",
    "ramda": "^0.29.0",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.15.10",
    "@types/ramda": "0.29.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "typescript": "5.0.2"
  }
}
